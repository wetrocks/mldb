{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "Enrich": [ ],
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '%swagger%'"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "mldbDB": "Data Source=localDb/sqlite.db",
    "pgConn": "Host=localhost;Port=5432;Username=postgres;Password=password;"
  }
}