{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Info",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "Enrich": [ ],
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '%swagger%'"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "mldbDB": "Data Source=localDb/sqlite.db"
  }
}